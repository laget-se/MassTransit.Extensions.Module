<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>MassTransit.Extensions.Module</id>
    <title>MassTransit.Extensions.Module</title>
    <version>$version$</version>
    <authors>laget.se</authors>
    <owners>laget.se</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Autofac inspired `Module` structure for `MassTransit` that is used by calling `.RegisterModule<T>()` in `.AddMassTransit`.</description>
    <summary></summary>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://laget.se</projectUrl>
    <icon>assets\icon.png</icon>
    <readme>assets\README.md</readme>
    <repository type="git" url="https://github.com/laget-se/MassTransit.Extensions.Module" branch="master" />
    <dependencies>
      <group targetFramework="net7.0">
        <dependency id="MassTransit.Extensions.DependencyInjection" version="7.3.1" />
      </group>
      <group targetFramework="net6.0">
        <dependency id="MassTransit.Extensions.DependencyInjection" version="7.3.1" />
      </group>
      <group targetFramework="netstandard2.1">
        <dependency id="MassTransit.Extensions.DependencyInjection" version="7.3.1" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="MassTransit.Extensions.DependencyInjection" version="7.3.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\README.md" target="assets\" />
    <file src="..\assets\icon.png" target="assets\" />
    <file src=".\bin\Release\net7.0\*" target="lib\net7.0\" />
    <file src=".\bin\Release\net6.0\*" target="lib\net6.0\" />
    <file src=".\bin\Release\netstandard2.1\*" target="lib\netstandard2.1\" />
    <file src=".\bin\Release\netstandard2.0\*" target="lib\netstandard2.0\" />
  </files>
</package>
